  "scripts": {
    "deploy": "yarn build && tar czf build_$(date +'%Y%m%d_%H%M%S').tar.gz -s /^public/copachampagnat/ public/*",
    "deploy:staging": "yarn build:staging && tar -C public/ -czf build_$(date +'%Y%m%d_%H%M%S').tar.gz  .",
    "deploy:dev": "yarn build:dev && tar czf build_$(date +'%Y%m%d_%H%M%S').tar.gz -s /^public// public/*",
    "build": "rm -rf ./public && GATSBY_ACTIVE_ENV=production gatsby build --prefix-paths",
    "build:staging": "rm -rf ./public && GATSBY_ACTIVE_ENV=staging gatsby build --prefix-paths",
    "build:dev": "rm -rf ./public && GATSBY_ACTIVE_ENV=development gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juliovedovatto/marista-copa-champagnat-frontend"
